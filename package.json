{
  "name": "oneapi-cli",
  "version": "0.6.0",
  "description": "一个用于替代 Swagger/SpringFox 的 API 工具，不需要修改后端代码，也不需要启动应用（几乎能 💯 替代）",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "bin": {
    "oneapi": "./bin/index.js"
  },
  "scripts": {
    "dev": "ts-node ./bin/index.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "test": "mocha --timeout 5000 -r ts-node/register \"test/**/*.test.ts\"",
    "ci": "nyc --reporter=lcov npm test",
    "prettier": "prettier --config .prettierrc --write .",
    "tsc": "rm -rf ./build && tsc && cp -r ./sdk ./build && cp package.json ./build && cp README.md ./build && rm -rf ./build/sdk/lib"
  },
  "dependencies": {
    "camelcase": "^6.0.0",
    "chalk": "^4.0.0",
    "commander": "^9.4.0",
    "fs-extra": "^10.1.0",
    "ts-morph": "^15.1.0",
    "tslib": "~2.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.23",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "~5.26",
    "@typescript-eslint/parser": "~5.26",
    "chai": "^4.3.6",
    "codecov": "^3.8.3",
    "eslint": "~8.16",
    "eslint-config-prettier": "~8.5",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "~2.6",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
